<%= form_with(model: student_path, local: true) do |form| %>
  <% if student_path.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(student_path.errors.count, "error") %> prohibited this student_path from being saved:</h2>

      <ul>
      <% student_path.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :student_id %>
    <%= collection_select(:student_path, :student_id, Student.all, :id, :email, {}, {:class =>'form-control'} )%>
  </div>

  <div class="field">
    <%= form.label :student_id %>
    <%= collection_select(:student_path, :path_id, Path.all, :id, :title, {}, {:class =>'form-control'} )%>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
